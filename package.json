{
  "name": "webpack-react-redux-workflow",
  "version": "0.0.1",
  "description": "A good starting point for creating React apps with webpack and redux",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf public/build",
    "start": "node --harmony_array_includes start.js",
    "dev": "better-npm-run dev",
    "prod": "npm run clean && better-npm-run prod",
    "no-devtools": "better-npm-run dev -- --no_devtools",
    "test": "npm run test"
  },
  "betterScripts": {
    "dev": {
      "command": "npm start",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000,
        "DEV_SERVER_PORT": 8080
      }
    },
    "prod": {
      "command": "npm start",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3000
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github/blaqbern/webpack-react-redux-workflow"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "hmr",
    "eslint",
    "css-modules"
  ],
  "author": "Chris Blackburn",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "0.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "http-proxy": "^1.14.0",
    "postcss-loader": "^0.8.1",
    "postcss-modules-values": "^1.1.1",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "redux-thunk": "^1.0.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1",
    "yargs": "^4.1.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "react": "^0.14.7"
  }
}
